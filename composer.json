{
    "name": "jgraffite/snake2camel",
    "description": "A Laravel Framework Package for getting and setting model fields in snakeCase through camelCase",
    "keywords": [
        "laravel",
        "models",
        "database fields",
        "jgraffite",
        "snake2case"
    ],
    "homepage": "https://github.com/jgraffite/snake2camel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Luis Malaquias",
            "email": "jorge@macrobol.com",
            "homepage_empty": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": ">=5.5",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Jgraffite\\Snake2camel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jgraffite\\Snake2camel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev": "1.1"
        }
    }
}
